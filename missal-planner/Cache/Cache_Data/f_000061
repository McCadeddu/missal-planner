    },
    [
      isResizing,
      viewportTop,
      computeVisibleLines,
      sendLiveThrottled,
      isLive
    ]
  );
  useEffect(() => {
    lineRefs.current = lineRefs.current.slice(0, lines.length);
  }, [lines.length]);
  const toggleLive = () => {
    if (!isLive) {
      const visible = updateViewportPreview();
      setIsLive(true);
      sendLiveThrottled({ text: visible, offset: viewportTop, live: true });
    } else {
      setIsLive(false);
      sendLiveThrottled({ text: "", offset: 0, live: false });
    }
  };
